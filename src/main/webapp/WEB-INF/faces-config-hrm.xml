<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <converter>
        <converter-id>activeManufacturerConverter</converter-id>
        <converter-class>jm.com.dpbennett.hrm.converter.ActiveManufacturerConverter</converter-class>
    </converter>
    <converter>
        <converter-id>activeEmployeePositionConverter</converter-id>
        <converter-class>jm.com.dpbennett.hrm.converter.ActiveEmployeePositionConverter</converter-class>
    </converter> 
    <converter>
        <converter-id>activeSubgroupConverter</converter-id>
        <converter-class>jm.com.dpbennett.hrm.converter.ActiveSubgroupConverter</converter-class>
    </converter>
    <converter>
        <converter-id>addressConverter</converter-id>
        <converter-class>jm.com.dpbennett.hrm.converter.AddressConverter</converter-class>
    </converter>
    <converter>
        <converter-id>businessOfficeConverter</converter-id>
        <converter-class>jm.com.dpbennett.hrm.converter.BusinessOfficeConverter</converter-class>
    </converter>      
    <converter>
        <converter-id>businessConverter</converter-id>
        <converter-class>jm.com.dpbennett.hrm.converter.BusinessConverter</converter-class>
    </converter>   
    <converter>
        <converter-id>manufacturerContactConverter</converter-id>
        <converter-class>jm.com.dpbennett.hrm.converter.ManufacturerContactConverter</converter-class>
    </converter>
    <converter>
        <converter-id>manufacturerAddressConverter</converter-id>
        <converter-class>jm.com.dpbennett.hrm.converter.ManufacturerAddressConverter</converter-class>
    </converter>  
    <validator>
        <validator-id>subcontractedDepartmentValidator</validator-id>
        <validator-class>jm.com.dpbennett.hrm.validator.SubcontractedDepartmentValidator</validator-class>
    </validator>    
    <validator>
        <validator-id>manufacturerValidator</validator-id>
        <validator-class>jm.com.dpbennett.hrm.validator.ManufacturerValidator</validator-class>
    </validator>      
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>employeePositionDialog</from-outcome>
            <to-view-id>/hr/employeePositionDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>employeeDialog</from-outcome>
            <to-view-id>/hr/employeeDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>departmentDialog</from-outcome>
            <to-view-id>/hr/departmentDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>departmentPickListDialog</from-outcome>
            <to-view-id>/hr/departmentPickListDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>   
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>employeePickListDialog</from-outcome>
            <to-view-id>/hr/employeePickListDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>  
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>subgroupPickListDialog</from-outcome>
            <to-view-id>/hr/subgroupPickListDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>businessDialog</from-outcome>
            <to-view-id>/hr/businessDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>subgroupDialog</from-outcome>
            <to-view-id>/hr/subgroupDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>divisionDialog</from-outcome>
            <to-view-id>/hr/divisionDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <!--
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>manufacturerDialog</from-outcome>
            <to-view-id>/hr/manufacturer/manufacturerDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    -->    
      
    <managed-bean>
        <managed-bean-name>humanResourceManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.hrm.manager.HumanResourceManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>    
    
    <managed-bean>
        <managed-bean-name>financeManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.fm.manager.FinanceManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
</faces-config>
